{
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "AuthService",
    "Audience": "Services",
    "Key": "development_only_key_never_use_in_production_minimum_64_characters_long_for_security"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName",
      "WithMachineName"
    ]
  }
}
